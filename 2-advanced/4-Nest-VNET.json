{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "Prefix": {
      "type": "string"
    },
    "vnetName": {
      "type": "string"
    },
    "vnetIPs": {
      "type": "string"
    },
    "nsg1Id": {
      "type": "string"
    },
    "location": {
      "type": "string"
    }
  },
  "functions": [],
  "variables": {
    "vnetSubIPs": "[parameters('vnetIPs')]"
  },
  "resources": [
    {
      "type": "Microsoft.Network/virtualNetworks",
      "apiVersion": "2020-06-01",
      "name": "[parameters('vnetName')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "addressSpace": {
          "addressPrefixes": "[parameters('vnetIPs')]"
        },
        "subnets": [
          {
            "name": "subnet1",
            "properties": {
              "addressPrefix": "[variables('vnetSubIPs')]",
              "networkSecurityGroup": {
                "id": "[parameters('nsg1Id')]"
              }
            }
          }
        ]
      }
    }
  ],
  "outputs": {
    "vnetId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName'))]"
    },
    "subnetId": {
      "type": "string",
      "value": "[format('{0}/subnets/{{vnetSubName}}', resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName')))]"
    }
  }
}